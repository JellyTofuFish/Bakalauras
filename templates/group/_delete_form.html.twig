<form class="w-49-5" method="post" action="{{ path('group_delete', {'id': group.id}) }}">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ group.id) }}">
    <button class="btn btn-block btn-outline-danger btn-no-top-radius btn-no-right-radius">{{ 'button.delete'|trans }}</button>
</form>
<button class="groupDeleteCancel w-49-5 btn btn-block btn-primary btn-no-top-radius btn-no-left-radius">{{ 'button.cancel'|trans }}</button>